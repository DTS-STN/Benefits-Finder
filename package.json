{
  "name": "benefits-finder",
  "version": "0.1.0",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test:unit": "jest",
    "test:coverage": "jest --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "lint": "prettier --write \"pages/**/*.{js,jsx,ts,tsx,json,md}\" --write \"lib/**/*.{js,jsx,ts,tsx,json,md}\" --write \"components/**/*.{js,jsx,ts,tsx,json,md}\" --write \"public/locales/**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint:formatting": "prettier --check \"pages/**/*.{js,jsx,ts,tsx,json,md}\" --check \"lib/**/*.{js,jsx,ts,tsx,json,md}\" --check \"components/**/*.{js,jsx,ts,tsx,json,md}\" --check \"public/locales/**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint:rules": "eslint ./",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.1",
    "@tailwindcss/forms": "^0.3.2",
    "@tailwindcss/line-clamp": "^0.2.1",
    "@tailwindcss/typography": "^0.4.0",
    "cookie": "^0.4.1",
    "focus-trap-react": "^8.5.0",
    "husky": "^4.3.0",
    "i18next-http-backend": "^1.2.2",
    "lint-staged": "^10.5.4",
    "next": "10.2.0",
    "next-i18next": "^8.2.0",
    "prettier": "^2.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-markdown": "^6.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/eslint-parser": "^7.14.3",
    "@storybook/addon-actions": "^6.2.1",
    "@storybook/addon-essentials": "^6.2.1",
    "@storybook/addon-links": "^6.2.1",
    "@storybook/react": "^6.2.1",
    "@testing-library/dom": "^7.30.2",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "autoprefixer": "^10.2.5",
    "axe-core": "^4.1.4",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "cypress": "^7.2.0",
    "cypress-axe": "^0.12.2",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fetch-mock": "^9.11.0",
    "jest": "^26.6.3",
    "jest-axe": "^4.1.0",
    "postcss": "^8.2.9",
    "tailwindcss": "^2.1.1"
  },
  "lint-staged": {
    "pages/**/*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write"
    ],
    "lib/**/*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write"
    ],
    "components/**/*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write"
    ],
    "pages/locales/**/*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write"
    ]
  }
}
