{
  "name": "benefits-finder",
  "version": "0.1.0",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test:unit": "jest",
    "test:coverage": "jest --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "lint": "prettier --write \"pages/**/*.{js,jsx,ts,tsx,json,md}\" --write \"lib/**/*.{js,jsx,ts,tsx,json,md}\" --write \"components/**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint:test": "prettier --check \"pages/**/*.{js,jsx,ts,tsx,json,md}\" --check \"lib/**/*.{js,jsx,ts,tsx,json,md}\" --check \"components/**/*.{js,jsx,ts,tsx,json,md}\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "husky": "^4.3.0",
    "lint-staged": "^10.5.4",
    "next": "10.1.2",
    "prettier": "^2.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@storybook/addon-actions": "^6.2.1",
    "@storybook/addon-essentials": "^6.2.1",
    "@storybook/addon-links": "^6.2.1",
    "@storybook/react": "^6.2.1",
    "@testing-library/dom": "^7.30.2",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "autoprefixer": "^10.2.5",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "cypress": "^6.8.0",
    "cypress-axe": "0.12.2",
    "jest": "^26.6.3",
    "jest-axe": "^4.1.0",
    "postcss": "^8.2.9",
    "tailwindcss": "^2.1.1"
  },
  "lint-staged": {
    "pages/**/*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write"
    ],
    "lib/**/*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write"
    ],
    "components/**/*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write"
    ]
  }
}
